{
  "name": "@zignaly-open/ui",
  "version": "0.6.18",
  "description": "A library of interface resources designed by Zignaly.",
  "module": "./lib/packages/zignaly-ui/src/index.js",
  "main": "./lib/cjs.js",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./icons": { "import": "./lib/icons.js" },
    "./fonts": { "import": "./lib/fonts.js" },
    "./charts": { "import": "./lib/charts.js" },
    ".": { "import": "./lib/index.js" }
  },
  "typesVersions": {
    "*": {
      "*": ["./lib/*", "./lib/index.d.ts"]
    }
  },
  "sideEffects": false,
  "scripts": {
    "prepare": "yarn build",
    "start": "rollup -cw",
    "storybook": "storybook dev -p 6006 -s ./src",
    "build:storybook": "storybook build -c .storybook -o build/",
    "build-storybook": "yarn test:generate-output && yarn build:storybook",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test:generate-output-with-watch": "jest --json --outputFile=.jest-test-results.json --watch || true",
    "test": "jest -u",
    "test:watch": "jest --watch",
    "build": "rollup -c",
    "copy-files": "cp -R src/assets lib/assets",
    "lint": "eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "lint-ci": "echo 'Unless you fix all disabled rules, you knwo what you are'; eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src --config ./.prettierrc",
    "chromatic": "npx chromatic --project-token=e5c345734579",
    "release:patch": "git tag -l | xargs git tag -d && git checkout releases && npm version patch && git push origin && git push origin --tags && npm run build && npm publish",
    "release:minor": "git tag -l | xargs git tag -d && git checkout releases && npm version minor && git push origin && git push origin --tags && npm run build && npm publish",
    "release:major": "git tag -l | xargs git tag -d && git checkout releases && npm version major && git push origin && git push origin --tags && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zignaly-open/react-component-library"
  },
